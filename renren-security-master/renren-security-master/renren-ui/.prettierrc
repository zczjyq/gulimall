{
  "singleQuote": false,
  "semi": true,
  "trailingComma": "none",
  "printWidth": 100,
  "arrowParens": "always",
  "tabWidth": 2,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    },
    {
      "files": "*.vue",
      "options": {
        "parser": "vue",
        "printWidth": 300
      }
    }
  ]
}
